{
  "name": "vue-router-webcache",
  "version": "1.0.8",
  "description": "Enable vue-router routing for google webcache(and same caches)",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist && rimraf test/fixtures/*/.nuxt",
    "build": "tsc",
    "postbuild": "copyfiles -u 2 lib/nuxt/router.js dist/nuxt",
    "dev": "nuxt dev test/fixtures/nuxt",
    "postversion": "git push --tags && yarn publish && git push && echo \"Successfully released version $npm_package_version!\"",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./lib/index.ts",
    "./nuxt": "./lib/nuxt/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kolobok12309/vue-router-webcache.git"
  },
  "keywords": [
    "router",
    "webcache",
    "nuxt-module",
    "vue"
  ],
  "author": "Kolobok12309 <sova123409@gmail.com>",
  "license": "MIT",
  "private": false,
  "homepage": "https://github.com/Kolobok12309/vue-router-webcache#readme",
  "peerDependencies": {
    "vue": "^2.6.14",
    "vue-router": "^3.5.2"
  },
  "dependencies": {
    "@nuxtjs/router": "^1.6.1",
    "consola": "^2.15.3",
    "defu": "^5.0.0",
    "lodash": "^4.17.21",
    "nuxt-vuex-router-sync": "^0.0.3",
    "rimraf": "^3.0.2",
    "url-parse": "^1.5.3"
  },
  "devDependencies": {
    "@nuxt/test-utils": "^0.2.2",
    "@nuxt/types": "^2.15.8",
    "@nuxtjs/proxy": "^2.1.0",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.173",
    "@types/node": "^16.9.2",
    "@types/url-parse": "^1.4.4",
    "@types/vue": "^2.0.0",
    "@types/vue-router": "^2.0.0",
    "copyfiles": "^2.4.1",
    "jest": "^27.3.1",
    "nuxt": "^2.15.8",
    "playwright": "^1.16.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.3"
  }
}
